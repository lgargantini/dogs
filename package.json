{
    "homepage": "https://lgargantini.github.io/dogs/",
    "name": "dogs",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "react": "^18.1.0",
        "react-dom": "^18.1.0",
        "react-scripts": "5.0.1"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --watchAll=false --no-cache --detectOpenHandles --logHeapUsage --runInBand",
        "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache --detectOpenHandles",
        "eject": "react-scripts eject",
        "predeploy": "react-scripts build",
        "deploy": "gh-pages -d build"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@eslint/js": "^9.6.0",
        "@testing-library/dom": "^10.2.0",
        "@testing-library/jest-dom": "^6.4.6",
        "@testing-library/react": "^16.0.0",
        "@types/babel__core": "^7.20.5",
        "@types/babel__traverse": "^7.20.6",
        "@types/bonjour": "^3.5.13",
        "@types/eslint__js": "^8.42.3",
        "@types/jest": "^29.5.12",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@types/trusted-types": "^2.0.7",
        "eslint": "^8.57.0",
        "eslint-plugin-jest": "^28.6.0",
        "eslint-plugin-react": "^7.34.3",
        "gh-pages": "^6.1.1",
        "globals": "^15.7.0",
        "husky": "^9.0.11",
        "lint-staged": "^15.2.5",
        "prettier": "^3.2.5",
        "typescript": "^4.5.5",
        "typescript-eslint": "^7.14.1"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
            "prettier --write"
        ]
    }
}
